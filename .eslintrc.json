{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "semi": "off",
        "indent": "off",
        "no-console":"off",
        "no-unused-vars": "off"
    }
}
